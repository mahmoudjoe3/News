<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="oval">
            <size android:height="@dimen/_16sdp"
                android:width="@dimen/_16sdp"/>

            <solid android:color="#ffffff" />
            <stroke
                android:dashGap="3dp"
                android:dashWidth="5dp"
                android:width="1dp"
                android:color="@color/colorBlack1" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
        </shape>
    </item>
</layer-list>